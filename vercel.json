{
  "version": 2,
  "routes": [
    {
      "src": "/prod-api/(.*)",
      "dest": "https://api.qzdcg.com/prod-api/$1"
    },
    {
      "src": "/admin-api/(.*)",
      "dest": "https://api.metass.top/admin-api/$1"
    },
    {
      "src": "/app-api/(.*)",
      "dest": "https://api.metass.top/app-api/$1"
    },
    {
      "src": "/v1/(.*)",
      "dest": "https://api.openai.com/v1/$1"
    }
  ]
}
